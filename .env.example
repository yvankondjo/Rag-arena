# LLM Configuration
# Provider: "openrouter" or "openai"
LLM_PROVIDER=openrouter
LLM_MODEL=gpt-4o-mini

# OpenRouter API Key (for OpenRouter provider)
OPENROUTER_API_KEY=sk_test_your_openrouter_key_here

# OpenAI API Key (for embeddings and OpenAI LLM provider)
OPENAI_API_KEY=sk-proj_your_openai_key_here

# Embedding Configuration
EMBEDDING_MODEL=text-embedding-3-small

# Dataset Configuration (optional)
DATASET_NAME=scifact
DATASET_SPLIT=test
NUM_QUERIES=100

# Retrieval Configuration (optional)
RETRIEVAL_MODE=dense
TOP_K=10

# Orchestration Configuration (optional)
ORCHESTRATION_MODE=simple
MAX_AGENTIC_STEPS=3

# Langfuse Observability (optional)
# Get your keys from https://cloud.langfuse.com/
LANGFUSE_PUBLIC_KEY=pk-lf_your_public_key
LANGFUSE_SECRET_KEY=sk-lf_your_secret_key
LANGFUSE_HOST=https://cloud.langfuse.com
